generator client {
  provider = "prisma-client-js"
  output   = "../prisma/generatedClient/"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
  userName  String
  nationId  String   @unique
}
