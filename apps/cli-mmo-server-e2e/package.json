{
  "name": "@cli-mmo/cli-mmo-server-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@cli-mmo/cli-mmo-server"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/cli-mmo-server-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@cli-mmo/cli-mmo-server:build",
          "@cli-mmo/cli-mmo-server:serve"
        ]
      }
    }
  }
}
